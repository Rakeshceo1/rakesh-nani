a=input("Enter the Boy name:")
b=input("Enter the Girl name: ")
a1=list(a)
b1=list(b)
for i in range(len(a1)):
    for j in range(len(b1)):
        if(a1[i]==b1[i]):
            a1[i]='2'
            b1[j]='2'
print(a1)
print(b1)
d=0
for i in a1:
    if(i!='2'):
        d=d+1
            
for i in b1:
    if(i!='2'):
        d=d+1
print(d)
f=0
res=list("FLAMES")
for i in range(5):
    
    f=(f+(d-1))%len(res)
    res.pop(f)
print(res[0])
        
    
